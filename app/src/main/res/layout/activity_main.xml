<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.nath.appgroup.MainActivity">

    <com.example.nath.appgroup.CustomImageView
        android:id="@+id/customImageView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <SeekBar
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:max="255"
        android:progress="1"
        android:visibility="invisible"
        android:id="@+id/seekBarColorFilter"
        android:layout_below="seekBarRadialBlur"
        android:layout_alignParentLeft="true" />

    <SeekBar
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:max="255"
        android:progress="1"
        android:visibility="invisible"
        android:id="@+id/seekBarContrastEqualization"
        android:layout_below="seekBarHoughTheta"
        android:layout_alignParentLeft="true" />

    <SeekBar
        android:id="@+id/seekBarColorize"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:max="255"
        android:progress="1"
        android:visibility="invisible"
        android:layout_alignParentStart="true"
        android:layout_below="seekBarCannyLow"
        android:layout_alignParentLeft="true" />

    <SeekBar
        android:id="@+id/seekBarLuminosity"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:max="255"
        android:progress="1"
        android:visibility="invisible"
        android:layout_alignParentStart="true"
        android:layout_below="seekBarSpheres"
        android:layout_alignParentLeft="true" />

    <SeekBar
        android:id="@+id/seekBarFlash"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:max="255"
        android:progress="1"
        android:visibility="invisible"
        android:layout_alignParentStart="true"
        android:layout_below="seekBarFlashlightRadius"
        android:layout_alignParentLeft="true" />

    <SeekBar
        android:id="@+id/seekBarSpheres"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:max="100"
        android:progress="1"
        android:visibility="invisible"
        android:layout_below="seekBarContrastEqualization"
        android:layout_alignParentLeft="true" />

    <SeekBar
        android:id="@+id/seekBarMosaic"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:max="100"
        android:progress="1"
        android:visibility="invisible"
        android:layout_alignParentBottom="true" />

    <SeekBar
        android:id="@+id/seekBarPainting"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:max="100"
        android:progress="1"
        android:visibility="invisible"
        android:layout_below="seekBarFlash"
        android:layout_alignParentLeft="true" />

    <SeekBar
        android:id="@+id/seekBarRadialBlur"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:max="100"
        android:progress="1"
        android:visibility="invisible"
        android:layout_alignParentStart="true"
        android:layout_below="seekBarLabyRatio"
        android:layout_alignParentLeft="true" />

    <SeekBar
        android:id="@+id/seekBarCircularBlur"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:max="100"
        android:progress="1"
        android:visibility="invisible"
        android:layout_alignParentStart="true"
        android:layout_below="seekBarMosaic"
        android:layout_alignParentLeft="true" />

    <SeekBar
        android:id="@+id/seekBarHoughTheta"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:max="255"
        android:progress="1"
        android:visibility="invisible"
        android:layout_alignParentStart="true"
        android:layout_below="seekBarCannyHigh"
        android:layout_alignParentLeft="true" />

    <SeekBar
        android:id="@+id/seekBarHoughThreshold"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:max="255"
        android:progress="1"
        android:visibility="invisible"
        android:layout_alignParentStart="true"
        android:layout_below="seekBarCircularBlur"
        android:layout_alignParentLeft="true" />

    <SeekBar
        android:id="@+id/seekBarCannyLow"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:max="255"
        android:progress="1"
        android:visibility="invisible"
        android:layout_alignParentStart="true"
        android:layout_below="seekBarHoughThreshold"
        android:layout_alignParentLeft="true" />

    <SeekBar
        android:id="@+id/seekBarLabyRatio"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:max="255"
        android:progress="1"
        android:visibility="invisible"
        android:layout_alignParentStart="true"
        android:layout_below="seekBarPainting"
        android:layout_alignParentLeft="true" />

    <SeekBar
        android:id="@+id/seekBarFlashlightRadius"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:max="255"
        android:progress="1"
        android:visibility="invisible"
        android:layout_alignParentStart="true"
        android:layout_below="seekBarColorize"
        android:layout_alignParentLeft="true" />

    <SeekBar
        android:id="@+id/seekBarCannyHigh"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:max="255"
        android:progress="1"
        android:visibility="invisible"
        android:layout_alignParentStart="true"
        android:layout_below="seekBarFlashlightIntensity"
        android:layout_alignParentLeft="true" />

    <SeekBar
        android:id="@+id/seekBarLabyMax"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:max="255"
        android:progress="1"
        android:visibility="invisible"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <SeekBar
        android:id="@+id/seekBarFlashlightIntensity"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:max="255"
        android:progress="1"
        android:visibility="invisible"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/textViewTop"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textViewMiddle"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="15dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/textViewMiddle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textViewBottom"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="16dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/textViewBottom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/seekBarMosaic"
        android:layout_alignParentStart="true"
        android:text="TextView" />

</RelativeLayout>